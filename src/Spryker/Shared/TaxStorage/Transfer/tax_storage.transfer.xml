<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="TaxSetStorage">
        <property name="id" type="string"/>
        <property name="uuid" type="string"/>
        <property name="key" type="string"/>
        <property name="name" type="string"/>
        <property name="fkTaxSet" type="int"/>
        <property name="data" type="TaxSetDataStorage"/>
    </transfer>

    <transfer name="TaxSetDataStorage">
        <property name="taxRates" type="TaxRateStorage[]" singular="taxRate"/>
    </transfer>

    <transfer name="TaxRateStorage">
        <property name="name" type="string"/>
        <property name="country" type="string"/>
        <property name="rate" type="float"/>
    </transfer>
</transfers>